{
  "name": "ngx-tru-forms-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod ngx-tru-forms",
    "build-dev": "ng build ngx-tru-forms",
    "publish": "npm run build && cd dist/ngx-tru-forms && npm publish",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish-git": "npm run clean && npm run build && deploy-to-git",
    "clean": "rm -fr build dist"
  },
  "config": {
    "deployToGit": {
      "repository": "git@github.com:trufla-technology/ngx-tru-forms.git",
      "branch": "gh-pages",
      "folder": "build",
      "script": "sh publish-git.sh",
      "commit": "Publishing $npm_package_version",
      "user": {
        "name": "Boris Cosic",
        "email": "boris.c@trufla.com"
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.6",
    "@angular/cdk": "^6.3.1",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.3.1",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "angular2-text-mask": "^9.0.0",
    "core-js": "^2.5.4",
    "deploy-to-git": "^0.1.5",
    "lodash.startcase": "^4.4.0",
    "moment": "^2.22.2",
    "ngx-color-picker": "^6.3.3",
    "rxjs": "^6.0.0",
    "text-mask-addons": "^3.7.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.6",
    "@angular-devkit/build-ng-packagr": "~0.6.6",
    "@angular/cli": "~6.0.7",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
