    "clean": "rm -fr build dist",
    "updatemat": "ng update  ngx-tru-forms @angular/material@8 --allow-dirty"
  },
  "config": {
    "deployToGit": {
      "repository": "git@github.com:trufla-technology/ngx-tru-forms.git",
      "branch": "gh-pages",
      "folder": "build",
      "commit": "Publishing $npm_package_version",
      "user": {
        "name": "Boris Cosic",
        "email": "boris.c@trufla.com"
      }
    }
  },
  "private": true,
  "dependencies": {
